{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "baseUrl": "./",
        "paths": {
            "*": ["node_modules/*"]
        }},
        "include": ["assets/**/*.ts", "assets/**/*.d.ts"],
        "exclude": ["node_modules"]
}
